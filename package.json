{
  "name": "full-stack-example-app",
  "version": "1.0.0",
  "description": "基于opentelemetry及天机阁的页面加载性能及接口耗时监控demo",
  "main": "src/server/index.js",
  "scripts": {
    "build": "webpack --mode production",
    "start": "npm run build && node src/server/index.js",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon src/server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "echoqlwang",
  "license": "ISC",
  "dependencies": {
    "ejs": "^3.1.6",
    "express": "^4.16.3",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "babel-polyfill": "^6.26.0",
    "@opentelemetry/api": "^1.0.3",
    "@opentelemetry/exporter-collector": "0.25.0",
    "@opentelemetry/exporter-jaeger": "0.25.0",
    "@opentelemetry/exporter-zipkin": "0.25.0",
    "@opentelemetry/instrumentation": "0.25.0",
    "@opentelemetry/instrumentation-document-load": "^0.24.0",
    "@opentelemetry/instrumentation-fetch": "0.25.0",
    "@opentelemetry/instrumentation-http": "0.25.0",
    "@opentelemetry/resources": "0.25.0",
    "@opentelemetry/sdk-trace-base": "0.25.0",
    "@opentelemetry/sdk-trace-node": "0.25.0",
